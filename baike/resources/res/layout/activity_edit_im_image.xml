<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="#000000" android:layout_width="match_parent" android:layout_height="match_parent">
    <qsbk.app.widget.ButtonBar android:id="@+id/id_top_bar" android:layout_width="wrap_content" android:layout_height="wrap_content" app:bgColor="#7f000000" app:rightTitle="确定"/>
    <qsbk.app.widget.imageview.TouchImageView android:id="@+id/multitouchimageview" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="fitCenter" android:layout_below="@+id/id_top_bar" android:layout_centerInParent="true"/>
    <ProgressBar android:id="@+id/progressbar" android:layout_width="wrap_content" android:layout_height="wrap_content" android:indeterminate="true" android:layout_centerInParent="true" style="@style/Widget.ProgressBar.Large.Inverse"/>
</RelativeLayout>
