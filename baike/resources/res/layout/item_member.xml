<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="67dp" style="?attr/nearby_list_item_bg">
    <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/avatar" android:layout_width="@dimen/middle_user_icon" android:layout_height="@dimen/middle_user_icon" android:layout_marginLeft="13dp" android:layout_marginRight="12dp" android:layout_centerVertical="true" app:actualImageScaleType="0"/>
    <TextView android:ellipsize="end" android:id="@+id/name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_marginRight="50dp" android:singleLine="true" android:layout_toRightOf="@+id/avatar" style="?attr/nearby_list_item_name"/>
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/gender_age" android:layout_width="wrap_content" android:layout_height="13dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:layout_toRightOf="@+id/avatar" android:layout_below="@+id/name" style="?attr/nearby_list_item_genderage">
        <ImageView android:id="@+id/gender" android:paddingLeft="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/pinfo_female" android:scaleType="fitXY" android:contentDescription="@null"/>
        <TextView android:textSize="9dp" android:gravity="center" android:id="@+id/age" android:paddingLeft="2dp" android:paddingRight="2dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true" android:includeFontPadding="false"/>
    </LinearLayout>
    <TextView android:textSize="9dp" android:textColor="#ffffff" android:gravity="center" android:id="@+id/role" android:background="@drawable/pinfo_female_bg" android:paddingLeft="5dp" android:paddingRight="5dp" android:layout_width="wrap_content" android:layout_height="13dp" android:layout_marginLeft="3dp" android:singleLine="true" android:includeFontPadding="false" android:layout_toRightOf="@+id/gender_age" android:layout_alignTop="@+id/gender_age"/>
    <TextView android:textSize="@dimen/g_txt_middle" android:textColor="#969696" android:id="@+id/me" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="13dp" android:text="æˆ‘" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
    <RelativeLayout android:id="@+id/action" android:paddingRight="7dp" android:paddingBottom="7dp" android:layout_width="50dp" android:layout_height="match_parent" android:layout_alignParentRight="true">
        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/member_more" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <View android:id="@+id/divider" android:background="#e4e4e4" android:layout_width="match_parent" android:layout_height="1dp" android:layout_alignParentBottom="true"/>
</RelativeLayout>
