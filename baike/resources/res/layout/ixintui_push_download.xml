<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="wrap_content">
    <ImageView android:id="@+id/ixintui_push_download_icon" android:layout_width="48dp" android:layout_height="48dp" android:layout_marginLeft="10dp" android:layout_marginTop="8dp" android:layout_marginRight="10dp" android:layout_marginBottom="8dp" android:contentDescription="@android:drawable/stat_sys_download"/>
    <TextView android:textAppearance="@android:style/TextAppearance.Medium" android:ellipsize="marquee" android:id="@+id/ixintui_push_download_title" android:fadingEdge="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="4dp" android:layout_marginTop="6dp" android:singleLine="true" android:layout_toRightOf="@+id/ixintui_push_download_icon" android:layout_alignParentTop="true"/>
    <ProgressBar android:id="@+id/ixintui_push_download_progress" android:layout_width="match_parent" android:layout_height="6dp" android:max="100" android:layout_below="@+id/ixintui_push_download_title" android:layout_alignLeft="@+id/ixintui_push_download_title" style="@style/Widget.ProgressBar.Horizontal"/>
    <TextView android:textAppearance="@android:style/TextAppearance.Small" android:id="@+id/ixintui_push_download_percent" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="4dp" android:layout_marginBottom="6dp" android:layout_below="@+id/ixintui_push_download_progress" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
    <TextView android:textAppearance="@android:style/TextAppearance.Small" android:ellipsize="marquee" android:id="@+id/ixintui_push_download_message" android:fadingEdge="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="4dp" android:layout_marginBottom="6dp" android:singleLine="true" android:layout_toLeftOf="@+id/ixintui_push_download_percent" android:layout_toRightOf="@+id/ixintui_push_download_icon" android:layout_below="@+id/ixintui_push_download_progress" android:layout_alignLeft="@+id/ixintui_push_download_title" android:layout_alignParentBottom="true"/>
</RelativeLayout>
