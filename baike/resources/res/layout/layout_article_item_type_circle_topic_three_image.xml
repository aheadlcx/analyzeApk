<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:descendantFocusability="blocksDescendants" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="8dp">
    <LinearLayout android:orientation="vertical" android:background="?attr/digger_bar_three_image_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="8dp">
        <RelativeLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp">
            <TextView android:textSize="13dp" android:textColor="?attr/topic_focus_color" android:gravity="center" android:layout_gravity="center" android:id="@+id/topic_btn" android:background="?attr/topic_focus_bg" android:layout_width="60dp" android:layout_height="28dp" android:layout_marginRight="@dimen/profile_item_default_margin" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            <TextView android:textSize="18dp" android:textColor="?attr/topic_title_color" android:ellipsize="end" android:gravity="left|center_vertical|center_horizontal|center" android:id="@+id/topic_title" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="@dimen/profile_item_default_margin" android:layout_marginRight="10dp" android:singleLine="true" android:includeFontPadding="false" android:layout_toLeftOf="@+id/topic_btn" android:layout_centerHorizontal="true"/>
        </RelativeLayout>
        <LinearLayout android:gravity="center" android:orientation="horizontal" android:paddingLeft="@dimen/profile_item_default_margin" android:paddingRight="@dimen/profile_item_default_margin" android:paddingBottom="20dp" android:layout_width="match_parent" android:layout_height="match_parent">
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/imageview1" android:layout_width="0dp" android:layout_height="104dp" android:layout_weight="1" app:actualImageScaleType="6"/>
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/imageview2" android:layout_width="0dp" android:layout_height="104dp" android:layout_marginLeft="4dp" android:layout_weight="1" app:actualImageScaleType="6"/>
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/imageview3" android:layout_width="0dp" android:layout_height="104dp" android:layout_marginLeft="4dp" android:layout_weight="1" app:actualImageScaleType="6"/>
        </LinearLayout>
    </LinearLayout>
    <RelativeLayout android:id="@+id/layerMask" android:background="@drawable/edit_info_items_card_mask" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="4dp"/>
</FrameLayout>
