<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <qsbk.app.widget.AspectRatioImageView android:id="@+id/video_preview" android:drawingCacheQuality="low" android:layout_width="match_parent" android:layout_height="match_parent" android:minHeight="60dp" android:layout_centerInParent="true" android:contentDescription="@null" app:resize_mode="2" app:actualImageScaleType="0"/>
