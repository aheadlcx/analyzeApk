<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/video_preview" android:drawingCacheQuality="low" android:layout_width="match_parent" android:layout_height="match_parent" android:minHeight="60dp" android:contentDescription="@null" app:actualImageScaleType="0"/>
    <qsbk.app.widget.NightModeMaskView android:layout_width="match_parent" android:layout_height="match_parent" android:layout_alignTop="@+id/image" android:layout_alignBottom="@+id/image"/>
    <ImageView android:id="@+id/video_play_flag" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="6dp" android:layout_marginBottom="5dp" android:src="@drawable/video_vol" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
    <TextView android:textSize="12dp" android:textColor="#ffffff" android:id="@+id/video_duration" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="6dp" android:layout_marginBottom="3dp" android:text="12:48" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
    <ProgressBar android:id="@+id/video_progress" android:visibility="gone" android:layout_width="46dp" android:layout_height="46dp" android:max="100" android:indeterminate="true" android:layout_centerInParent="true" style="?attr/video_loading_progress"/>
</merge>
