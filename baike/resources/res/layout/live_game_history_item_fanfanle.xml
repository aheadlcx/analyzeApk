<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="23dp">
    <ImageView android:layout_gravity="center_vertical" android:id="@+id/tab_new" android:visibility="gone" android:layout_width="30dp" android:layout_height="14dp" android:layout_marginLeft="4dp" android:src="@drawable/live_game_history_new_ic" android:scaleType="fitXY"/>
    <LinearLayout android:gravity="center_vertical" android:layout_gravity="center" android:orientation="horizontal" android:id="@+id/ll_history_bg" android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:textSize="@dimen/live_text_12sp" android:id="@+id/tv_win" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="9dp" android:layout_weight="1"/>
        <TextView android:textSize="@dimen/live_text_12sp" android:textColor="#407b5a" android:gravity="center" android:layout_gravity="center" android:id="@+id/tv_result" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <TextView android:textSize="@dimen/live_text_12sp" android:textColor="#407b5a" android:gravity="right" android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/tv_time" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="13dp" android:layout_weight="2"/>
    </LinearLayout>
</FrameLayout>
