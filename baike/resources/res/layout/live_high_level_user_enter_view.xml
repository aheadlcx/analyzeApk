<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/container" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <FrameLayout android:id="@+id/fl_enter_v2" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <qsbk.app.core.widget.FrameAnimationView android:layout_gravity="center_vertical" android:id="@+id/iv_frame_anim" android:layout_width="375dp" android:layout_height="75dp"/>
        <ViewFlipper android:id="@+id/vf_desc" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="83dp" android:layout_marginTop="30dp" android:inAnimation="@anim/anim_bottom_in" android:outAnimation="@anim/anim_top_out" android:alpha="0">
            <TextView android:textSize="@dimen/live_text_10sp" android:textStyle="bold" android:textColor="@color/white" android:id="@+id/tv_family" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            <LinearLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:textSize="@dimen/live_text_11sp" android:textColor="@color/white" android:ellipsize="middle" android:id="@+id/live_level_v2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="210dp" android:text="LV12" android:singleLine="true"/>
                <TextView android:textSize="@dimen/live_text_11sp" android:textStyle="bold" android:textColor="@color/white" android:ellipsize="middle" android:id="@+id/live_name_v2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="2dp" android:maxWidth="210dp" android:text="haha" android:singleLine="true"/>
                <TextView android:textSize="@dimen/live_text_11sp" android:textColor="@color/white" android:ellipsize="middle" android:id="@+id/live_content_v2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="2dp" android:maxWidth="210dp" android:text="来了" android:singleLine="true"/>
            </LinearLayout>
        </ViewFlipper>
    </FrameLayout>
    <FrameLayout android:id="@+id/fl_enter" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <RelativeLayout android:gravity="center_vertical" android:layout_gravity="center" android:id="@+id/rl_high_level" android:layout_width="match_parent" android:layout_height="29dp">
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/sd_high_level1" android:background="@drawable/live_level_bg_left" android:layout_width="47dp" android:layout_height="match_parent" android:layout_alignParentLeft="true" app:placeholderImageScaleType="2"/>
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/sd_high_level3" android:background="@drawable/live_level_bg_right" android:layout_width="47dp" android:layout_height="match_parent" android:layout_alignParentRight="true" app:placeholderImageScaleType="2"/>
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/sd_high_level2" android:background="@drawable/live_level_bg_middle" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_toLeftOf="@+id/sd_high_level3" android:layout_toRightOf="@+id/sd_high_level1" app:placeholderImageScaleType="2"/>
        </RelativeLayout>
        <com.facebook.drawee.view.SimpleDraweeView android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/iv_live_left_anim" android:layout_width="127dp" android:layout_height="86dp" android:layout_marginLeft="13dp"/>
        <com.facebook.drawee.view.SimpleDraweeView android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/iv_live_right_anim" android:layout_width="127dp" android:layout_height="86dp" android:layout_marginRight="13dp"/>
        <FrameLayout android:gravity="center_vertical" android:layout_gravity="center" android:id="@+id/fl_user_info" android:paddingTop="2dp" android:paddingBottom="2dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="35dp" android:layout_marginTop="4dp" android:layout_marginRight="35dp" android:layout_marginBottom="4dp">
            <LinearLayout android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <TextView android:textSize="@dimen/live_text_16sp" android:ellipsize="middle" android:gravity="center_vertical" android:layout_gravity="center" android:id="@+id/live_level" android:paddingLeft="21dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="210dp" android:text="LV12" android:singleLine="true"/>
                <TextView android:textSize="@dimen/live_text_13sp" android:textStyle="bold" android:ellipsize="middle" android:gravity="center_vertical" android:layout_gravity="center" android:id="@+id/live_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="210dp" android:text="haha" android:singleLine="true"/>
                <TextView android:textSize="@dimen/live_text_13sp" android:ellipsize="middle" android:gravity="center_vertical" android:layout_gravity="center" android:id="@+id/live_content" android:paddingRight="21dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxWidth="210dp" android:text="来了" android:singleLine="true"/>
            </LinearLayout>
            <ImageView android:layout_gravity="left|center_vertical|center_horizontal|center" android:id="@+id/live_enter_highlight" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/live_high_level_user_enter_highlight"/>
        </FrameLayout>
        <TextView android:textSize="@dimen/live_text_12sp" android:textColor="#ffe485" android:layout_gravity="bottom|center_vertical|center_horizontal|center" android:id="@+id/live_barrage_bg" android:background="@drawable/live_enterview_badge_bg" android:paddingLeft="15dp" android:paddingTop="1dp" android:paddingRight="15dp" android:paddingBottom="1dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="18dp" android:text="徽章"/>
    </FrameLayout>
</FrameLayout>
