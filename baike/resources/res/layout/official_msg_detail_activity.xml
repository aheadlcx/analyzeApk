<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:SettingItem="http://schemas.android.com/apk/res/qsbk.app" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/id_fake_actionbar" android:background="?attr/actionbar" android:layout_width="match_parent" android:layout_height="8dp" android:layout_marginTop="-3dp">
        <ProgressBar android:id="@+id/progressbar" android:layout_width="match_parent" android:layout_height="8dp" android:maxHeight="8dp" android:indeterminate="false" android:progressDrawable="@drawable/progress_horizontal_qiustore" android:minHeight="8dp" style="@style/Widget.AppCompat.ProgressBar.Horizontal"/>
    </FrameLayout>
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="-4dp">
        <qsbk.app.core.web.ui.QsbkWebView android:id="@+id/webview" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <View android:id="@+id/webview_mask" android:background="#77000000" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_alignTop="@+id/webview" android:layout_alignBottom="@+id/webview"/>
        <FrameLayout android:id="@+id/fl_video" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent"/>
    </RelativeLayout>
</LinearLayout>
