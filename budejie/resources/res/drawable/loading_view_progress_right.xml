<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dp"/>
            <gradient android:startColor="@color/video_seek_bar_bg" android:endColor="@color/video_seek_bar_bg" android:angle="90"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:gravity="left" android:clipOrientation="horizontal">
            <shape>
                <corners android:radius="0dp"/>
                <gradient android:startColor="#ffffff" android:endColor="#ffffff" android:angle="90"/>
            </shape>
        </clip>
    </item>
</layer-list>
