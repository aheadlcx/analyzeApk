<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3dp"/>
            <gradient android:startColor="#656666" android:endColor="#dbdedf" android:angle="270" android:centerY="0.75" android:centerColor="#bbbbbc"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3dp"/>
                <gradient android:startColor="#464647" android:endColor="#2d9ae7" android:angle="270"/>
            </shape>
        </clip>
    </item>
</layer-list>
