<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dp"/>
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="90" android:centerColor="#ffffffff"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dp"/>
                <gradient android:startColor="#ffff2d55" android:endColor="#ffff2d55" android:angle="90" android:centerColor="#ffff2d55"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dp"/>
                <gradient android:startColor="#ff00ff00" android:endColor="#ff00ff00" android:angle="90" android:centerColor="#ff00ff00"/>
            </shape>
        </clip>
    </item>
</layer-list>
