<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto" android:id="@+id/rl_root" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/rl_content" android:background="@drawable/dialog_user_info_bg" android:visibility="visible" android:clickable="true" android:layout_width="290dp" android:layout_height="wrap_content" android:layout_centerInParent="true">
        <TextView android:textSize="12sp" android:textColor="@color/black_2" android:id="@+id/report" android:padding="13dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/user_info_report"/>
        <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/iv_header" android:layout_width="68dp" android:layout_height="68dp" android:layout_marginTop="16dp" android:layout_centerHorizontal="true" fresco:placeholderImage="@drawable/rooms_third_common_head_portrait" fresco:roundAsCircle="true"/>
        <TextView android:textSize="15sp" android:textColor="#222222" android:id="@+id/tv_user_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:layout_below="@+id/iv_header" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="12sp" android:textColor="#999999" android:id="@+id/tv_user_rid" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="3dp" android:text="(1111111)" android:layout_below="@+id/tv_user_name" android:layout_centerHorizontal="true"/>
        <RelativeLayout android:gravity="center" android:id="@+id/rl_user_badge" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="3dp" android:layout_below="@+id/tv_user_rid" android:layout_centerHorizontal="true">
            <ImageView android:id="@+id/iv_vip" android:layout_width="17dp" android:layout_height="17dp" android:src="@drawable/rooms_third_common_head_portrait" android:layout_centerVertical="true"/>
            <ImageView android:id="@+id/iv_card" android:layout_width="17dp" android:layout_height="17dp" android:layout_marginLeft="5dp" android:src="@drawable/rooms_third_common_head_portrait" android:layout_toRightOf="@+id/iv_vip" android:layout_centerVertical="true"/>
            <cn.v6.sixrooms.room.view.DraweeTextView android:id="@+id/tv_badge" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_toRightOf="@+id/iv_card"/>
        </RelativeLayout>
        <FrameLayout android:id="@+id/fl_profile_covering" android:layout_width="60dp" android:layout_height="100dp" android:layout_centerHorizontal="true"/>
        <RelativeLayout android:id="@+id/rl_info" android:paddingTop="3dp" android:layout_width="match_parent" android:layout_height="55dp" android:layout_below="@+id/rl_user_badge">
            <RelativeLayout android:id="@+id/rl_star_level_info" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="20dp" android:layout_marginRight="20dp" android:layout_alignParentBottom="true">
                <ImageView android:id="@+id/iv_level" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/rooms_third_anchor_level_1" android:scaleType="center" android:layout_centerVertical="true"/>
                <cn.v6.sixrooms.ui.view.UserInfoProgressBar android:id="@+id/user_level_progressBar" android:layout_width="135dp" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:progress="0" android:layout_toLeftOf="@+id/iv_level_next" android:layout_toRightOf="@+id/iv_level" android:layout_centerVertical="true"/>
                <ImageView android:id="@+id/iv_level_next" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/rooms_third_anchor_level_1" android:scaleType="center" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                <TextView android:textSize="12sp" android:textColor="#999999" android:id="@+id/tv_level_desc" android:paddingBottom="3dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" android:layout_above="@+id/user_level_progressBar" android:layout_centerHorizontal="true"/>
            </RelativeLayout>
            <RelativeLayout android:id="@+id/rl_coin_level_info" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="20dp" android:layout_marginRight="20dp" android:layout_alignParentBottom="true">
                <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/iv_coin_level" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/rooms_third_anchor_level_1" android:scaleType="center" android:layout_centerVertical="true"/>
                <cn.v6.sixrooms.ui.view.UserInfoProgressBar android:id="@+id/user_coin_level_progressBar" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:layout_marginRight="5dp" android:progress="0" android:layout_toLeftOf="@+id/iv_coin_level_next" android:layout_toRightOf="@+id/iv_coin_level" android:layout_centerVertical="true"/>
                <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/iv_coin_level_next" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/rooms_third_anchor_level_1" android:scaleType="center" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                <TextView android:textSize="12sp" android:textColor="#999999" android:id="@+id/tv_coin_level_desc" android:paddingBottom="3dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true" android:layout_above="@+id/user_coin_level_progressBar" android:layout_centerHorizontal="true"/>
            </RelativeLayout>
            <View android:id="@+id/ll_bottom2_line" android:background="#e8e8e8" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_bottom_2" android:background="@drawable/userinfo_dialog_btn_left_right_selecter" android:layout_width="match_parent" android:layout_height="38dp" android:layout_below="@+id/rl_info">
            <TextView android:textSize="12sp" android:textColor="@color/dialog_user_info_follow_color" android:gravity="center" android:id="@+id/tv_public_chat" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="对Ta说" android:layout_weight="1"/>
            <View android:id="@+id/div7_2" android:background="#e8e8e8" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="@color/dialog_user_info_follow_color" android:gravity="center" android:id="@+id/tv_gift" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="送礼" android:layout_weight="1"/>
            <View android:id="@+id/div8_2" android:background="#e8e8e8" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="@color/dialog_user_info_follow_color" android:gravity="center" android:id="@+id/tv_private_chat" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="私聊" android:layout_weight="1"/>
            <View android:id="@+id/div4_2" android:background="#e8e8e8" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="#333333" android:gravity="center" android:id="@+id/tv_mute_2" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="gone" android:layout_width="0dp" android:layout_height="match_parent" android:text="禁言" android:layout_weight="1"/>
            <View android:id="@+id/div5_2" android:background="#e8e8e8" android:visibility="gone" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="#333333" android:gravity="center" android:id="@+id/tv_popup_2" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="gone" android:layout_width="0dp" android:layout_height="match_parent" android:text="踢人" android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_bottom" android:background="@drawable/userinfo_dialog_btn_left_right_selecter" android:layout_width="match_parent" android:layout_height="38dp" android:layout_below="@+id/ll_bottom_2">
            <TextView android:textSize="12sp" android:textColor="#333333" android:gravity="center" android:id="@+id/tv_upgrade_admin" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="升管理" android:layout_weight="1"/>
            <View android:id="@+id/div3_2" android:background="#e8e8e8" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="#333333" android:gravity="center" android:id="@+id/tv_upgrade_manager" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="升总管" android:layout_weight="1"/>
            <View android:id="@+id/div1" android:background="#e8e8e8" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="#333333" android:gravity="center" android:id="@+id/tv_mute" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:layout_width="0dp" android:layout_height="match_parent" android:text="禁言" android:layout_weight="1"/>
            <View android:id="@+id/div2" android:background="#e8e8e8" android:layout_width="0.5dp" android:layout_height="match_parent" android:layout_marginTop="12dp" android:layout_marginBottom="12dp"/>
            <TextView android:textSize="12sp" android:textColor="#333333" android:gravity="center" android:id="@+id/tv_popup" android:background="@drawable/userinfo_dialog_btn_middle_selecter" android:layout_width="0dp" android:layout_height="match_parent" android:text="踢人" android:layout_weight="1"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_bottom3" android:background="@drawable/userinfo_dialog_btn_left_right_selecter" android:layout_width="match_parent" android:layout_height="38dp" android:layout_below="@+id/ll_bottom">
            <TextView android:textSize="12sp" android:textColor="@color/dialog_user_info_follow_color" android:gravity="center" android:id="@+id/tv_follow" android:background="@drawable/userinfo_dialog_btn_left_right_selecter" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="关注" android:layout_weight="1"/>
        </LinearLayout>
        <ImageView android:id="@+id/iv_close_btn" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/dialog_close_selector" android:layout_alignParentTop="true" android:layout_alignParentRight="true"/>
        <ImageView android:id="@+id/ll_bottom_line" android:background="#e8e8e8" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_below="@+id/ll_bottom_2"/>
        <ImageView android:id="@+id/ll_bottom_line3" android:background="#e8e8e8" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_below="@+id/ll_bottom"/>
    </RelativeLayout>
    <RelativeLayout android:gravity="center_horizontal" android:id="@+id/progressBar" android:visibility="gone" android:layout_width="150dp" android:layout_height="30dp" android:layout_centerInParent="true">
        <ProgressBar android:id="@+id/progress" android:layout_width="25dp" android:layout_height="25dp" android:layout_marginLeft="10dp" style="@style/CustomProgressBarStyleLarge"/>
        <TextView android:textSize="15sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/text_loading" android:layout_width="100dp" android:layout_height="25dp" android:layout_marginLeft="11dp" android:layout_marginRight="10dp" android:text="加载中..." android:layout_alignParentRight="true"/>
    </RelativeLayout>
</RelativeLayout>
