<?xml version="1.0" encoding="utf-8"?>
<cn.v6.sixrooms.widgets.phone.UserManagerView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res/com.budejie.www" android:orientation="vertical" android:id="@+id/smoothView" android:background="@color/transparent" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:id="@+id/video_show_ll" android:background="@android:color/black" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <FrameLayout android:id="@+id/ll_option" android:background="#ffffff" android:layout_width="match_parent" android:layout_height="@dimen/thirty_five_dip" android:layout_below="@+id/video_show_ll">
            <View android:layout_gravity="bottom" android:background="#26000000" android:layout_width="match_parent" android:layout_height="0.5dp"/>
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent" android:baselineAligned="false">
                <RelativeLayout android:id="@+id/rl_public_chat" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="2">
                    <View android:id="@+id/top_line1" android:background="#f43e26" android:layout_width="50dp" android:layout_height="3dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
                    <ImageView android:id="@+id/iv_chat_common" android:background="@drawable/rooms_third_room_public_chat" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:contentDescription="@null"/>
                    <TextView android:textSize="@dimen/text_size_small_14" android:textColor="@color/room_second_titlebar_textcolor_checked" android:gravity="center" android:id="@+id/tv_chat_common" android:paddingBottom="@dimen/three_dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_chat_common" android:layout_centerInParent="true"/>
                </RelativeLayout>
                <RelativeLayout android:id="@+id/rl_private_chat" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="2">
                    <ImageView android:id="@+id/iv_chat_private" android:background="@drawable/rooms_third_room_private_chat_no" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:contentDescription="@null"/>
                    <TextView android:textSize="@dimen/text_size_small_14" android:textColor="@color/room_second_titlebar_textcolor_unchecked" android:gravity="center" android:id="@+id/tv_chat_private" android:paddingBottom="@dimen/three_dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_chat_private" android:layout_centerInParent="true"/>
                    <ImageView android:id="@+id/iv_private_new_msg" android:background="@drawable/rooms_third_phone_room_private_new_msg" android:paddingRight="@dimen/five_dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="2dp" android:layout_marginBottom="-4dp" android:layout_toRightOf="@+id/tv_chat_private" android:layout_above="@+id/tv_chat_private" android:contentDescription="@null"/>
                    <View android:id="@+id/top_line2" android:background="#f43e26" android:visibility="invisible" android:layout_width="50dp" android:layout_height="3dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
                </RelativeLayout>
                <RelativeLayout android:gravity="center_horizontal" android:id="@+id/rl_spectator" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="2">
                    <View android:id="@+id/top_line3" android:background="#f43e26" android:visibility="invisible" android:layout_width="50dp" android:layout_height="3dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
                    <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent">
                        <ImageView android:id="@+id/iv_spectator" android:background="@drawable/rooms_third_room_spectator_no" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:contentDescription="@null"/>
                        <TextView android:textSize="@dimen/text_size_small_14" android:textColor="@color/room_second_titlebar_textcolor_unchecked" android:id="@+id/tv_spectator" android:paddingBottom="@dimen/three_dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_spectator"/>
                        <TextView android:textSize="@dimen/text_size_small_twelve" android:textColor="@color/gray" android:id="@+id/tv_spectator_num" android:paddingBottom="@dimen/three_dip" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="-5dp"/>
                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout android:gravity="center_horizontal" android:id="@+id/rl_more" android:layout_width="0dp" android:layout_height="match_parent" android:layout_weight="2">
                    <View android:id="@+id/top_line4" android:background="#f43e26" android:visibility="invisible" android:layout_width="50dp" android:layout_height="3dp" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
                    <ImageView android:id="@+id/iv_more" android:background="@drawable/rooms_third_room_more_no" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:contentDescription="@null"/>
                    <TextView android:textSize="@dimen/text_size_small_14" android:textColor="@color/room_second_titlebar_textcolor_unchecked" android:gravity="center" android:id="@+id/tv_more" android:paddingBottom="@dimen/three_dip" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_more" android:layout_centerInParent="true"/>
                </RelativeLayout>
                <RelativeLayout android:id="@+id/rl_followPart" android:background="@drawable/activity_room_attention_add_selector" android:visibility="gone" android:layout_width="60dp" android:layout_height="match_parent" android:layout_marginRight="-1dp">
                    <TextView android:textSize="@dimen/text_size_small_twelve" android:textColor="@color/white" android:gravity="right" android:id="@+id/tv_attentionStatus" android:focusable="true" android:clickable="false" android:layout_width="38dp" android:layout_height="wrap_content" android:text="@string/add_follow" android:layout_centerInParent="true"/>
                    <ImageView android:id="@+id/iv_attentionStatusIconPlus" android:focusable="false" android:visibility="visible" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="2dp" android:src="@drawable/activity_room_attetion_plus" android:layout_alignLeft="@+id/tv_attentionStatus" android:layout_centerVertical="true"/>
                </RelativeLayout>
            </LinearLayout>
        </FrameLayout>
        <include android:id="@+id/rl_bottom_items" android:layout_width="match_parent" android:layout_height="50dp" android:layout_alignParentBottom="true" layout="@layout/room_bottom"/>
        <RelativeLayout android:id="@+id/user_model_chat" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_above="@+id/rl_bottom_items" android:layout_below="@+id/ll_option"/>
        <RelativeLayout android:id="@+id/user_model_more" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/ll_option"/>
        <RelativeLayout android:id="@+id/giftPage" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_alignParentBottom="true"/>
        <RelativeLayout android:id="@+id/musicPage" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_alignParentBottom="true"/>
        <RelativeLayout android:id="@+id/fansPage" android:layout_width="match_parent" android:layout_height="@dimen/room_fans_height" android:layout_alignParentBottom="true"/>
        <RelativeLayout android:id="@+id/guardPage" android:layout_width="match_parent" android:layout_height="@dimen/room_fans_height" android:layout_alignParentBottom="true"/>
        <cn.v6.sixrooms.widgets.phone.PublicMenuWidgets android:layout_width="match_parent" android:layout_height="match_parent">
            <RelativeLayout android:id="@+id/public_menu_wrapper" android:background="@android:color/white" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="210dp" android:layout_alignParentBottom="true">
                <RelativeLayout android:id="@+id/public_menu_name_wrapper" android:background="@color/white" android:layout_width="match_parent" android:layout_height="@dimen/public_menu_name_wrapper_height">
                    <View android:id="@+id/view_border" android:background="@drawable/viewer_horizontal_broder" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_alignParentTop="true"/>
                    <TextView android:textSize="@dimen/text_size_middle_min" android:textColor="@color/public_menu_name_text_color" android:gravity="center" android:id="@+id/public_menu_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/public_menu_close" android:padding="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="@dimen/ten_dip" android:src="@drawable/public_menu_close_selector" android:scaleType="matrix" android:layout_alignParentRight="true" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
                </RelativeLayout>
                <View android:id="@+id/horizontal_top_border" android:background="@drawable/viewer_horizontal_broder" android:layout_width="match_parent" android:layout_height="0.5dp" android:layout_below="@+id/public_menu_name_wrapper"/>
                <cn.v6.sixrooms.widgets.phone.GiftGridView android:layout_gravity="center" android:id="@+id/public_menu" android:background="@color/viewer_list_normal_color_v4" android:layout_width="match_parent" android:layout_height="@dimen/public_menu_width" android:listSelector="@color/white" android:cacheColorHint="@color/white" android:stretchMode="columnWidth" android:numColumns="5" android:layout_below="@+id/horizontal_top_border"/>
            </RelativeLayout>
        </cn.v6.sixrooms.widgets.phone.PublicMenuWidgets>
    </RelativeLayout>
    <FrameLayout android:id="@+id/showGiftPage" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_alignParentBottom="true"/>
    <cn.v6.sixrooms.surfaceanim.view.AnimSurfaceViewTouch android:id="@+id/anim_surface_view" android:layout_width="match_parent" android:layout_height="match_parent"/>
</cn.v6.sixrooms.widgets.phone.UserManagerView>
