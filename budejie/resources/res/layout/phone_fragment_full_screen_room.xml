<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res/com.budejie.www" android:orientation="vertical" android:id="@+id/root_view" android:layout_width="match_parent" android:layout_height="match_parent">
    <include android:layout_width="match_parent" android:layout_height="@dimen/room_top_height" layout="@layout/room_include_top_host"/>
    <RelativeLayout android:id="@+id/room_root_rl" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:id="@+id/mini_game_layout" android:background="@color/mini_game_bg" android:visibility="gone" android:layout_width="match_parent" android:layout_height="180dp" android:layout_alignParentBottom="true">
            <cn.v6.sixrooms.room.view.MiniGameWebview android:id="@+id/game_webview" android:layout_width="match_parent" android:layout_height="180dp" android:layout_alignParentBottom="true" android:layerType="software"/>
            <ImageView android:id="@+id/mini_game_close" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="20dp" android:layout_marginBottom="146dp" android:src="@drawable/mini_game_close" android:layout_alignParentRight="true" android:layout_alignParentBottom="true"/>
        </RelativeLayout>
        <cn.v6.sixrooms.room.view.CustomSofaView android:id="@+id/room_custom_sofa" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:layout_marginRight="15dp" android:layout_alignParentRight="true"/>
        <TextView android:textSize="10sp" android:textColor="@color/white" android:gravity="center_vertical" android:id="@+id/tv_live_red_count" android:background="@drawable/bg_red_num_room_v6" android:paddingLeft="23dp" android:paddingRight="10dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginTop="11dp" android:text=""/>
        <RelativeLayout android:id="@+id/elogo_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="7dp" android:layout_marginRight="7dp">
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/iv_elogo" android:visibility="gone" android:layout_width="63dp" android:layout_height="47dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
            <com.facebook.drawee.view.SimpleDraweeView android:id="@+id/iv_elogo_two" android:visibility="gone" android:layout_width="63dp" android:layout_height="47dp" android:layout_marginTop="5dp" android:layout_marginBottom="5dp" android:layout_below="@+id/iv_elogo"/>
        </RelativeLayout>
        <ImageView android:id="@+id/warter_mark_iv" android:visibility="gone" android:layout_width="40dp" android:layout_height="20dp" android:layout_marginTop="40dp" android:layout_marginRight="16dp" android:src="@drawable/water_mark" android:layout_alignParentTop="true" android:layout_alignParentRight="true"/>
        <RelativeLayout android:id="@+id/pig_pk_duck_layout" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
        <RelativeLayout android:orientation="vertical" android:id="@+id/ll_bottom_wrapper" android:layout_width="match_parent" android:layout_height="@dimen/room_chat_height_fit" android:layout_alignParentBottom="true">
            <RelativeLayout android:id="@+id/rl_menu_down" android:background="@drawable/room_buttom_shadow" android:paddingLeft="@dimen/phone_sc_4dp" android:paddingRight="@dimen/phone_sc_4dp" android:layout_width="match_parent" android:layout_height="@dimen/phone_sc_44dp" android:layout_alignParentBottom="true">
                <ImageView android:id="@+id/iv_msg" android:layout_width="@dimen/phone_sc_44dp" android:layout_height="@dimen/phone_sc_44dp" android:src="@drawable/bt_msg_room_v6_selector"/>
                <ImageView android:id="@+id/iv_private_msg" android:layout_width="@dimen/phone_sc_44dp" android:layout_height="@dimen/phone_sc_44dp" android:layout_marginLeft="@dimen/room_private_chat_marginleft" android:src="@drawable/bt_msg_private_room_v6_selector" android:layout_toRightOf="@+id/iv_msg"/>
                <ImageView android:id="@+id/iv_gift" android:layout_width="@dimen/phone_sc_44dp" android:layout_height="@dimen/phone_sc_44dp" android:src="@drawable/bt_gift_room_v6_selector" android:layout_centerInParent="true"/>
                <ImageView android:id="@+id/iv_more" android:visibility="visible" android:layout_width="@dimen/phone_sc_44dp" android:layout_height="@dimen/phone_sc_44dp" android:src="@drawable/bt_more_room_v6_selector" android:layout_alignParentRight="true"/>
                <ImageView android:id="@+id/iv_share" android:visibility="invisible" android:layout_width="@dimen/phone_sc_44dp" android:layout_height="@dimen/phone_sc_44dp" android:layout_marginRight="@dimen/leftmenu_item_margin" android:src="@drawable/share_msg_room_v6_selector" android:layout_toLeftOf="@+id/iv_more"/>
            </RelativeLayout>
            <FrameLayout android:id="@+id/fl_chart" android:background="@color/transparent" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginRight="@dimen/room_chat_layout_marginright" android:layout_marginBottom="@dimen/room_chat_marginbottom" android:layout_above="@+id/rl_menu_down"/>
            <ImageView android:id="@+id/room_chat_top_misty" android:visibility="visible" android:layout_width="match_parent" android:layout_height="wrap_content" android:src="@drawable/room_publish_chat_top" android:scaleType="fitXY"/>
            <RelativeLayout android:id="@+id/send_red_layout" android:visibility="visible" android:layout_width="@dimen/phone_sc_44dp" android:layout_height="@dimen/phone_sc_44dp" android:layout_marginRight="4dp" android:layout_marginBottom="6dp" android:layout_above="@+id/rl_menu_down" android:layout_alignParentRight="true">
                <ImageView android:id="@+id/iv_send_red" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/bt_red_room_v6_selector" android:layout_centerInParent="true"/>
                <TextView android:textSize="10sp" android:textColor="@color/white" android:gravity="center" android:id="@+id/tv_red_num" android:background="@drawable/red_num" android:layout_width="16dp" android:layout_height="16dp" android:layout_marginLeft="-20dp" android:layout_marginBottom="-20dp" android:text="0" android:layout_toRightOf="@+id/iv_send_red" android:layout_above="@+id/iv_send_red"/>
            </RelativeLayout>
            <ImageView android:id="@+id/mini_game_open" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:layout_marginBottom="10dp" android:src="@drawable/mini_game_open" android:layout_above="@+id/send_red_layout" android:layout_alignParentRight="true"/>
        </RelativeLayout>
        <include android:layout_width="match_parent" android:layout_height="match_parent" layout="@layout/room_full_screen_private_chat_page"/>
    </RelativeLayout>
</LinearLayout>
