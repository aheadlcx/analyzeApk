<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res/com.budejie.www" android:id="@+id/root_rl" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:id="@+id/guard_title_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_centerHorizontal="true">
        <TextView android:textSize="15sp" android:textColor="#cdcdcd" android:gravity="center" android:id="@+id/guard_title" android:background="@drawable/room_guard_titlebg" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="哈哈哈开通了金守护"/>
        <ImageView android:id="@+id/guard_title_iv" android:paddingLeft="5dp" android:paddingRight="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/room_guard_title_iv"/>
    </RelativeLayout>
    <RelativeLayout android:id="@+id/guard_entiey_rl" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-50dp" android:layout_centerHorizontal="true">
        <ImageView android:id="@+id/guard_light_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="60dp" android:src="@drawable/room_guard_gold_light" android:layout_centerHorizontal="true"/>
        <ImageView android:id="@+id/guard_right_wing_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="40dp" android:layout_marginTop="-20dp" android:src="@drawable/room_guard_gold_right_wing" android:layout_alignLeft="@+id/guard_light_iv"/>
        <ImageView android:id="@+id/guard_left_wing_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="-20dp" android:layout_marginRight="40dp" android:src="@drawable/room_guard_gold_left_wing" android:layout_alignRight="@+id/guard_light_iv"/>
        <ImageView android:id="@+id/guard_entiey_bg_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="158dp" android:src="@drawable/room_guard_gold_entiey_bg" android:layout_centerHorizontal="true"/>
        <ImageView android:id="@+id/guard_entiey_light_iv" android:layout_width="wrap_content" android:layout_height="30dp" android:layout_marginTop="130dp" android:src="@drawable/room_guard_gold_entiey_light" android:layout_centerHorizontal="true"/>
        <ImageView android:id="@+id/guard_entiey_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="110dp" android:src="@drawable/room_guard_gold_entiey" android:layout_centerHorizontal="true"/>
        <ImageView android:id="@+id/guard_stars_point_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="80dp" android:src="@drawable/room_guard_stars_point_001" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</RelativeLayout>
