<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto" xmlns:app="http://schemas.android.com/apk/res/com.budejie.www" android:orientation="vertical" android:id="@+id/umeng_xp_web_main" android:background="#ffff" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:gravity="center_vertical" android:id="@+id/umeng_xp_actionBar" android:background="@drawable/umeng_xp_detail_bg" android:layout_width="match_parent" android:layout_height="40dp" android:layout_alignParentTop="true">
        <Button android:textSize="14sp" android:textColor="#ffffff" android:id="@+id/umeng_xp_cancel" android:background="@drawable/umeng_xp_back_button" android:paddingLeft="8dp" android:layout_width="50dp" android:layout_height="32dp" android:layout_marginLeft="8dp" android:text="@string/umeng_xp_back" android:layout_centerVertical="true"/>
        <TextView android:textSize="24sp" android:textStyle="bold" android:textColor="#ffffff" android:gravity="center" android:layout_width="wrap_content" android:layout_height="match_parent" android:text="@string/umeng_xp_title_info" android:layout_centerInParent="true"/>
    </RelativeLayout>
    <WebView android:id="@+id/webView" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/umeng_xp_actionBar"/>
    <RelativeLayout android:id="@+id/umeng_xp_loading_view" android:background="@android:color/darker_gray" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_below="@+id/umeng_xp_actionBar">
        <ProgressBar android:id="@+id/umeng_xp_loading" android:layout_width="28dp" android:layout_height="28dp" android:indeterminateDrawable="@drawable/umeng_xp_progressbar" android:layout_centerInParent="true"/>
        <SeekBar android:id="@+id/umeng_xp_loading_progress" android:visibility="gone" android:layout_width="186dp" android:layout_height="10dp" android:layout_marginTop="10dp" android:max="100" android:progressDrawable="@drawable/umeng_xp_loading_seek" android:thumb="@null" android:layout_below="@+id/umeng_xp_loading" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</RelativeLayout>
