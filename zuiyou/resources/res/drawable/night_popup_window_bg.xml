<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:left="2dp" android:top="2dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="#0f713131" android:endColor="#0f332a2a" android:angle="270"/>
            <corners android:topLeftRadius="6dp" android:topRightRadius="6dp" android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp"/>
        </shape>
    </item>
    <item android:right="3dp" android:bottom="3dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/night_primary_second_color" android:endColor="@color/night_primary_second_color" android:angle="270"/>
            <corners android:topLeftRadius="6dp" android:topRightRadius="6dp" android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp"/>
        </shape>
    </item>
</layer-list>
